- Tipe Data
#tipe data string
nama = "fitria sari"
print (nama)
#tipe data integer
umur = 20
print (umur)
#tipe data fload
tinggi = 1.60
print (tinggi)
#tipe data boolean
menikah = False
print (menikah)

- Operator Aritmatika
nilai1 = 6
nilai2 = 8
#penjumlahan
print (nilai1 + nilai2)
#pengurangan
print (nilai1 - nilai2)
#perkalian
print (nilai1 * nilai2)
#pembagian
print (nilai1 / nilai2)
#sisa bagi
print (nilai1 % nilai2)
#pangkat 
print (nilai1 ** nilai2)
#pembagian bulat
print (nilai1 // nilai2)

- Operator Penugasan
#sama dengan
nilai = 3
print (nilai)
#tambah sama dengan
nilai += 5
print (nilai)
#kurang sama dengan
nilai -= 4
print (nilai)
#kali sama dengan
nilai *= 8
print (nilai)
#bagi sama dengan
nilai /= 9
print (nilai)
#sisa bagi sama dengan
nilai %= 2
print (nilai)
#pangkat sama dengan
nilai **= 6
print (nilai)
#pembagian bulat sama dengan
nilai //= 7
print (nilai)

- Operator Perbandingan
#sama dengan 
print (3 == 7)
#tidak sama dengan
print (3 != 7)
#lebih besar dari
print (3 > 7)
#lebih kecil dari
print (3 < 7)
#lebih besar sama dengan
print (3 >= 7)
#lebih kecil sama dengan
print (3 <= 7)

-Operator Logika
#and
x = True
y = False
print (x and y)
#or 
x = True
y = False
print (x or y)
#not
x = True
print (not x)